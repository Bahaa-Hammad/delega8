{
  "name": "autom8",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test": "jest --config ./test/jest.config.ts",
    "test:tools": "jest --config ./test/jest.config.ts test/tools/tools.e2e-spec.ts",
    "test:tools:crud": "jest --config ./test/jest.config.ts test/api/tools/tools-crud-invoke.e2e-spec.ts",
    "test:tools:publish": "jest --config ./test/jest.config.ts test/api/tools/tools-publish-template.e2e-spec.ts",
    "test:steps": "jest --config ./test/jest.config.ts test/steps",
    "test:agents": "jest --config ./test/jest.config.ts test/api/agents",
    "test:agent:template": "jest --config ./test/jest.config.ts test/api/agents/templates",
    "test:agent:publish:extended": "jest --config ./test/jest.config.ts test/api/agents/agent-publish-clone-extended.e2e-spec.ts",
    "test:agent:chats": "jest --config ./test/jest.config.ts test/api/agents/chats.e2e-spec.ts",
    "test:agent:publish": "jest --config ./test/jest.config.ts test/api/agents/agent-publish.e2e-spec.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:drop": "npm run typeorm schema:drop -d src/config/datasource.ts",
    "schema:sync": "npm run typeorm schema:sync"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.744.0",
    "@crawlee/playwright": "^3.12.2",
    "@google-cloud/storage": "^7.15.1",
    "@langchain/anthropic": "^0.3.12",
    "@langchain/community": "^0.3.34",
    "@langchain/core": "^0.3.42",
    "@langchain/deepseek": "^0.0.1",
    "@langchain/google-genai": "^0.1.8",
    "@langchain/langgraph": "^0.2.40",
    "@langchain/openai": "^0.3.17",
    "@langchain/pinecone": "^0.1.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@pinecone-database/pinecone": "^4.1.0",
    "bcrypt": "^5.1.1",
    "chromadb": "^2.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parse": "^5.6.0",
    "d3-dsv": "^2.0.0",
    "dotenv": "^16.4.7",
    "isolated-vm": "^5.0.3",
    "json-schema-ref-parser": "^9.0.9",
    "langchain": "^0.3.11",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.79.0",
    "papaparse": "^5.5.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.1",
    "playwright": "^1.50.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-encrypted": "^0.8.0",
    "youtubei.js": "^12.2.0"
